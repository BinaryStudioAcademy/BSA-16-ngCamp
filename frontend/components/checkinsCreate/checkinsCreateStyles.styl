.checkins-setup-wrapper
    margin 5%
    li
        list-style none
    .checkins-setup
        display flex
        flex-flow row wrap
        justify-content center
        max-width 650px
        height 650px
        text-align center
        margin 0 auto
        background-color #f5f5f5
        border 1px solid #adadad
        border-radius 10px
        & > *
            flex 1 100%
    .header-setup, .main-setup, .list
        border-bottom 1px solid #adadad
    .header-setup
        height 18%
    .main-setup
        height 60%
        overflow auto
    .user-list
        li
            font-size 25px
            padding 1%
    .list
        display flex
        list-style none
        justify-content space-between
        margin-right 50px
    .footer-setup
        height 22%
